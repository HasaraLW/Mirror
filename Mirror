       class Mirror{

        public void pushdata(Stack<Integer> stack){

        for(int i = 0; i < 5; i++)
        {
           stack.push(i);
        }
        }

        public void popdata(Stack<Integer> stack){

        for(int i = 0; i < 5; i++)
        {
            Integer y = (Integer) stack.pop();
            System.out.println(y);
        }
        }
        public static void main(String args[]){

                String word = "ABCGTPPTGCZK";

                Stack<Integer> stack =new Stack<Integer>();

                pushdata(stack);
                popdata(stack);
        }

}
         
